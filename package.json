{
  "name": "yerevan",
  "version": "1.1.0",
  "description": "Node, TypeScript, Knex, PostgreSQL Starter Kit",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "npm run build:live",
    "build:live": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts",
    "build": "tsc && npm run lint",
    "clean": "rimraf ./lib",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "tsc": "tsc",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alonavns/Yerevan.git"
  },
  "keywords": [
    "Node",
    "TypeScript",
    "Knex",
    "Postgresql",
    "Starter",
    "Kit"
  ],
  "author": "Alona Veresehaka",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^10.12.10",
    "eslint": "^5.9.0",
    "nodemon": "^1.18.7",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1",
    "typescript-eslint-parser": "^21.0.1"
  },
  "bugs": {
    "url": "https://github.com/alonavns/Yerevan/issues"
  },
  "homepage": "https://github.com/alonavns/Yerevan",
  "dependencies": {
    "@decorators/common": "^1.1.2",
    "@decorators/di": "^1.0.2",
    "@decorators/express": "^2.3.0",
    "@types/express": "^4.16.0",
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "knex": "^0.15.2",
    "pg": "^7.7.1"
  }
}
